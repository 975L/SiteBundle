{# <twig:c975LSite:Image:Link [label=""] src="your-path/file.ext" url="yourdomain.ext" [class="your-css-classes"] [alt=""] width="width" height="height" [target=""]/> #}
{% set class = class | default('') %}
{% set alt = alt | default('') %}
{% set label = label | default('') %}
{% set target = target | default('') %}
<p>
    <a href="{{ url }}" target="{{ target }}"><img src="{{asset(src) }}" class="img img-responsive {{ class }}" alt="{{ alt }}" width="{{ width }}" height="{{ height }}" loading="lazy">{% if label != '' %}<br>{{ label }}{% endif %}</a>
</p>