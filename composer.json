{
    "name": "c975l/site-bundle",
    "type": "symfony-bundle",
    "description": "Groups common files and settings to create a website",
    "keywords": ["symfony", "bundle", "website"],
    "homepage": "https://github.com/975L/SiteBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Marquet",
            "email": "laurent.marquet@laposte.net",
            "homepage": "https://github.com/Laurent3170",
            "role": "Developer"
        },
        {
            "name": "975L Community",
            "homepage": "https://github.com/975L"
        }
    ],
    "require": {
        "php": "^7",
        "c975l/config-bundle": "^2",
        "c975l/includelibrary-bundle": "^1",
        "c975l/services-bundle": "^1",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/security": "*"
    },
    "require-dev": {
        "rector/rector": "^0.3",
        "symplify/easy-coding-standard": "^5.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "c975L\\SiteBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "email": "contact@975l.com",
        "source": "https://github.com/975L/SiteBundle",
        "issues": "https://github.com/975L/SiteBundle/issues"
    }
}
